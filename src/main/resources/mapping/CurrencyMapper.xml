<mapper namespace="com.mg.sn.mill.mapper.CurrencyMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mg.sn.mill.model.entity.Currency">
        <id column="ID" property="id" />
